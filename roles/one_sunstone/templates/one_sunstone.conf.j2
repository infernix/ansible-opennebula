# {{ ansible_managed }}
<VirtualHost *:80>
  ServerName sunstone-server
  DocumentRoot /usr/lib/one/sunstone/public
  PassengerUser oneadmin
  <Directory /usr/lib/one/sunstone/public>
     # This relaxes Apache security settings.
     AllowOverride all
     # MultiViews must be turned off.
     Options -MultiViews
  </Directory>
</VirtualHost>
